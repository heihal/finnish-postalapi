{
  "openapi": "3.0.3",
  "info": {
    "title": "Finnish Postal API",
    "version": "1.0.0"
  },
  "paths": {
    "/v1/lookup/{postal_code}": {
      "get": {
        "summary": "Lookup a Finnish Postal Code",
        "operationId": "lookup-postal-code",
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "urlForwardHandler",
            "module": "$import(@zuplo/runtime)",
            "options": {
              "url": "https://finnish-api-suite.onrender.com/api/v1/lookup/{params.postal_code}"
            }
          },
          "policies": {
            "inbound": [
              "api-key-inbound-1",
              "rate-limit-inbound-1"
            ]
          }
        }
      }
    }
  }
}